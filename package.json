{
  "name": "react-webpack-node",
  "version": "1.2.0",
  "description": "A simple Node.js app using Express 4 with Webpack, React, alt, ImmutableJS",
  "repository": "https://github.com/choonkending/react-webpack-node",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node server/index.js",
    "dev": "NODE_ENV=dev nodemon server/index.js",
    "devHotLoader": "npm run buildHotLoader && NODE_ENV=devhotloader node --harmony server/index.js",
    "build": "webpack --progress --colors --config ./webpack/webpack.config.js",
    "buildHotLoader": "webpack --progress --colors --config ./webpack/webpack-hot-dev-server.config.js",
    "watch": "webpack --watch --progress --config ./webpack/webpack.config.js",
    "watchHotLoader": "webpack --watch --progress --config ./webpack/webpack-hot-dev-server.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "webpack -p --config ./webpack/webpack.config.js"
  },
  "author": "Choon Ken Ding",
  "license": "MIT",
  "devDependencies": {
    "react-hot-loader": "^2.0.0-alpha-4",
    "webpack-dev-server": "^1.10.1"
  },
  "engines": {
    "node": "0.10.x"
  },
